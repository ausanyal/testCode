+syserr_t
+itr_ext_handle_int_timeout(itr_ext_per_hop_info_t *hop_info) 
+{
+    // destroy interval timer
+    tg_destroy_timer(hop_info->interval_timer);
+
+    ITR_DEBUG_PRINT(DBG_ITR_TRACE, "interval timeout handler");
+#ifdef ITR_EXT_DEBUG
+    ITR_EXT_PRINT("*********** Interval timeout handler  ***********");

+                      input->batch_id, input->bt_tep_ip, prev_hop->my_ttl, 
+                      prev_hop->hop_start_ts, prev_hop->src_port, 
+                      prev_hop->dst_port, hop_info->my_input, hop_info->my_cfg);
+    }
+
+    ITR_EXT_PRINT("*********** Current Hop_info ***********");
+    uint64  diff = hop_info->hop_end_ts - hop_info->hop_start_ts;

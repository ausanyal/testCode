+void 
+itr_ext_dump_hop(itr_ext_ht_entry_t *ht_entry, itr_ext_per_hop_info_t *hop_info, 
+                 itr_ext_input_t *input)
+{
+    ITR_EXT_PRINT("*********** All hops ***********");
+    GList* cursor = g_list_first(ht_entry->itr_ext_val.probe_list);
+    while(cursor) {

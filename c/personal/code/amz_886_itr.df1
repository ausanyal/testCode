+ bt_tep: 167838211
+ bt_tep: 167838983
+ */
+syserr_t
+itr_ext_debug_load_cfg_from_file(itr_ext_config_t *itr_ext_config)
+{
+    FILE *fp;
+    char s[50], line[100];
+    int i, j;
+
+    fp = fopen("/var/tmp/itr_ext_cfg.txt", "r");
+    if (fp == NULL) {
+        ITR_DEBUG_PRINT(DBG_ITR_ERROR, "Read Err: /tmp/itr_ext_cfg.txt");
+        return FAILURE;
+    }
+
+    while(fgets(line, 100, fp) != NULL) {
+        sscanf(line, "%s %d", s, &i);
+        ITR_DEBUG_PRINT(DBG_ITR_TRACE, "s:%s => i:%d", s, i);
+
+        if(!strcmp(s, "num_paths")) {
+            itr_ext_config->num_paths = i;
+        } 
+       if(!strcmp(s, "sport_start")) {
+            itr_ext_config->sport_start = i;
+        }
+        if(!strcmp(s, "sport_end")) {
+            itr_ext_config->sport_end = i;
+        }
+        if(!strcmp(s, "dport_start")) {
+            itr_ext_config->dport_start = i;
+        }
+        if(!strcmp(s, "dport_end")) {
+            itr_ext_config->dport_end = i;
+        } 
+        if(!strcmp(s, "prot:")) {
+            itr_ext_config->trace_cfg.prot = i; 
+        }
+        if(!strcmp(s, "int_sec:")) {
+            itr_ext_config->trace_cfg.probe_int_ttl_sec = i; 
+        }
+        if(!strcmp(s, "int_usec:")) {
+            itr_ext_config->trace_cfg.probe_int_ttl_usec = i; 
+        }
+        if(!strcmp(s, "ttl_sec:")) {
+            itr_ext_config->trace_cfg.per_hop_ttl_sec = i; 
+        }
+        if(!strcmp(s, "ttl_usec:")) {
+            itr_ext_config->trace_cfg.per_hop_ttl_usec = i; 
+        }
+        if(!strcmp(s, "fake_sec:")) {
+            itr_ext_config->trace_cfg.fake_hop_sec = i; 
+        }
+        if(!strcmp(s, "fake_usec:")) {
+            itr_ext_config->trace_cfg.fake_hop_usec = i; 
+        }
+        if(!strcmp(s, "max_hops:")) {
+            itr_ext_config->trace_cfg.max_hops = i; 
+        }
+        if(!strcmp(s, "num_bts:")) {
+            itr_ext_config->num_bts = i;
+            for(j = 0; j < itr_ext_config->num_bts ; j++) {
+                if (fgets(line, 100, fp) != NULL) {
+                    sscanf(line, "%s %d", s, &i);
+                    if(!strcmp(s, "bt_tep:")) {
+                        itr_ext_config->bt_teps[j] = i;
+                    }
+                    else {
+                        ITR_DEBUG_PRINT(DBG_ITR_ERROR, "Not enough bt_teps");
+                        return FAILURE;
+                    }
+                }
+                else {
+                    ITR_DEBUG_PRINT(DBG_ITR_ERROR, "Not enough info");
+                    return FAILURE;
+                }
+            }
+        }
+
+    }
+
+    return SUCCESS;
+}
